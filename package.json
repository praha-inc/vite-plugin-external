{
  "name": "@praha/vite-plugin-external",
  "version": "1.1.1",
  "description": "A Vite plugin that automatically excludes Node.js built-in modules and npm dependencies from the bundle",
  "keywords": [
    "javascript",
    "typescript",
    "vite",
    "plugin"
  ],
  "homepage": "https://github.com/praha-inc/vite-plugin-external/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/praha-inc/vite-plugin-external/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praha-inc/vite-plugin-external.git"
  },
  "license": "MIT",
  "author": "PrAha inc.",
  "type": "module",
  "imports": {
    "#package.json": "./package.json"
  },
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    },
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit",
    "version": "changeset",
    "version:apply": "changeset version",
    "version:publish": "changeset publish",
    "version:prepublish": "pkg-pr-new publish --pnpm --compact"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "@praha/eslint-config-common": "4.0.3",
    "@praha/eslint-config-definer": "2.0.2",
    "@praha/eslint-config-javascript": "3.0.8",
    "@praha/eslint-config-style": "3.0.9",
    "@praha/eslint-config-typescript": "3.0.9",
    "@rslib/core": "0.17.0",
    "@tsconfig/strictest": "2.0.7",
    "@types/node": "22.18.13",
    "eslint": "9.38.0",
    "memfs": "4.50.0",
    "pkg-pr-new": "0.0.60",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0"
  },
  "packageManager": "pnpm@10.20.0",
  "publishConfig": {
    "provenance": true
  }
}
